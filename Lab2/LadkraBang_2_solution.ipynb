   "source": [
    "trials = 10000\n",
    "successes = 0\n",
    "\n",
    "for _ in np.arange(trials):\n",
    "    rolls = np.random.choice(dice, k)\n",
    "    if sum(rolls == 6) &gt; 0:\n",
    "        successes = successes + 1\n",
    "        \n",
    "successes / trials"
   ]
